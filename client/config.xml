<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>channer</name>
    <description>
        channer client
    </description>
    <author email="iyatomi@gmail.com" href="https://github.com/pvac">
        channer dev
    </author>
    <content src="index.html" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <plugin name="cordova-plugin-battery-status" spec="~1.1.0" />
    <plugin name="cordova-plugin-camera" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-contacts" spec="~1.1.0" />
    <plugin name="cordova-plugin-device" spec="~1.0.1" />
    <plugin name="cordova-plugin-device-motion" spec="~1.1.1" />
    <plugin name="cordova-plugin-device-orientation" spec="~1.0.1" />
    <plugin name="cordova-plugin-dialogs" spec="~1.1.1" />
    <plugin name="cordova-plugin-file" spec="~3.0.0" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.3.0" />
    <plugin name="cordova-plugin-geolocation" spec="~1.0.1" />
    <plugin name="cordova-plugin-globalization" spec="~1.0.1" />
    <plugin name="cordova-plugin-inappbrowser" spec="~1.0.1" />
    <plugin name="cordova-plugin-media" spec="~1.0.1" />
    <plugin name="cordova-plugin-media-capture" spec="~1.0.1" />
    <plugin name="cordova-plugin-network-information" spec="~1.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="~2.1.0" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-vibration" spec="~1.2.0" />
    <plugin name="phonegap-plugin-contentsync" spec="~1.2.2" />
    <plugin name="phonegap-plugin-push" spec="~1.5.3" />
    <plugin name="cordova-plugin-whitelist" spec="~1.0.0" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="ios">
        <icon height="76" src="src/res/icon/ios/icon-76.png" width="76" />
        <icon height="152" src="src/res/icon/ios/icon-76@2x.png" width="152" />
        <icon height="72" src="src/res/icon/ios/icon-72.png" width="72" />
        <icon height="144" src="src/res/icon/ios/icon-72@2x.png" width="144" />
        <icon height="60" src="src/res/icon/ios/icon-60.png" width="60" />
        <icon height="120" src="src/res/icon/ios/icon-60@2x.png" width="120" />
        <icon height="180" src="src/res/icon/ios/icon-60@3x.png" width="180" />
        <icon height="50" src="src/res/icon/ios/icon-50.png" width="50" />
        <icon height="100" src="src/res/icon/ios/icon-50@2x.png" width="100" />
        <icon height="40" src="src/res/icon/ios/icon-40.png" width="40" />
        <icon height="80" src="src/res/icon/ios/icon-40@2x.png" width="80" />
        <icon height="57" src="src/res/icon/ios/icon.png" width="57" />
        <icon height="114" src="src/res/icon/ios/icon@2x.png" width="114" />
        <icon height="29" src="src/res/icon/ios/icon-small.png" width="29" />
        <icon height="58" src="src/res/icon/ios/icon-small@2x.png" width="58" />
        <splash height="480" src="src/res/splash/ios/Default~iphone.png" width="320" />
        <splash height="960" src="src/res/splash/ios/Default@2x~iphone.png" width="640" />
        <splash height="1024" src="src/res/splash/ios/Default-Portrait~ipad.png" width="768" />
        <splash height="2048" src="src/res/splash/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="768" src="src/res/splash/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="1536" src="src/res/splash/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="1136" src="src/res/splash/ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" src="src/res/splash/ios/Default-667h.png" width="750" />
        <splash height="2208" src="src/res/splash/ios/Default-736h.png" width="1242" />
        <splash height="1242" src="src/res/splash/ios/Default-Landscape-736h.png" width="2208" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
        <preference name="SplashScreenDelay" value="0" />
        <icon density="ldpi" src="src/res/icon/android/icon-36.png" />
        <icon density="mdpi" src="src/res/icon/android/icon-48.png" />
        <icon density="hdpi" src="src/res/icon/android/icon-72.png" />
        <icon density="xhdpi" src="src/res/icon/android/icon-96.png" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="wp8">
        <icon height="99" src="src/res/icon/wp8/ApplicationIcon.png" width="99" />
        <icon height="159" src="src/res/icon/wp8/TileMedium.png" width="159" />
        <icon height="110" src="src/res/icon/wp8/TileSmall.png" width="110" />
    </platform>
    <access origin="*" />
    <engine name="wp8" spec="~3.8.1" />
    <engine name="ios" spec="~3.9.2" />
    <engine name="android" spec="~4.1.1" />
</widget>
